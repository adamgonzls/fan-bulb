{{#each this}}
	<div data-id="{{id}}" class="tile">
		<div class="tile-image">
			Image here
		</div>
		<div class="tile-details">
			<div class="tile-details1">{{brand}}</div>
			<div class="tile-details2">{{model}}</div>
			<div class="control">
				<button>Info</button>
				<button>Compare</button>
			</div>
		</div>
	</div>
{{/each}}


